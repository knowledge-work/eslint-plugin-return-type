    "@typescript-eslint/rule-tester": "7.18.0",
    "eslint": "8.57.1",
    "jest": "30.1.3",
    "npm-run-all2": "6.2.6",
    "prettier": "3.6.2",
    "prettier-config-kwork": "0.0.33",
    "rimraf": "5.0.10",
    "ts-jest": "29.4.2",
    "typescript": "5.9.2"
