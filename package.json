  "version": "1.0.34",
  "repository": {
    "type": "git",
    "url": "https://github.com/knowledge-work/eslint-plugin-return-type.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
